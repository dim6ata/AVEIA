<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aveia">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_aveia_final"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_aveia_final_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AVEIA">
        <activity android:name=".SelectInterval" />
        <activity android:name=".CompleteInterval" />
        <activity android:name=".NoteTutorial" />
        <activity android:name=".XYPadActivity" />
        <activity android:name=".ThereminActivity" />
        <activity android:name=".InstrumentBaseActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".StartupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>